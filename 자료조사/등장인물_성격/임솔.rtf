\f1\b0\fs24 \cf0 \uc0\u44480 \u50685 \u44256  \u49324 \u46993 \u49828 \u47101 \u45796 . \u49905 \u44536 \u47084 \u50880 \u51060  \u49324 \u46988 \u51004 \u47196  \u53468 \u50612 \u45212 \u45796 \u47732  \u50500 \u47560  \u44536 \u45376 \u51032  \u47784 \u49845 \u51068 \u44620 ? \u54644 \u49324 \u54616 \u44172  \u50883 \u51012  \u46412 \u47732  \u48388 \u48341  \u44057 \u51008  \u50728 \u44592 \u44032  \u45712 \u44788 \u51652 \u45796 .\
\uc0\u50689 \u54868 \u44048 \u46021 \u51060  \u45000 \u51060 \u50632 \u51648 \u47564 , 15\u45380  \u51204  \u48520 \u51032 \u51032  \u49324 \u44256 \u47196  \u54616 \u48152 \u49888  \u47560 \u48708  \u54032 \u51221 \u51012  \u48155 \u44256  \u45000 \u51012  \u51217 \u50632 \u45796 . \u54788 \u51116 \u45716  \u50500 \u47476 \u48148 \u51060 \u53944 \u47196  \u50976 \u53916 \u48652  \u50689 \u49345  \u54200 \u51665  \u51068 \u51012  \u54616 \u47732 \u49436  \u50689 \u54868  \u54200 \u51665 \u51088 \u44032  \u46104 \u44592  \u50948 \u54644  \u45432 \u47141  \u51473 \u51064 \u45936  \u50668 \u47084  \u50689 \u54868  \u51228 \u51089 \u49324 \u50640  \u51064 \u53556  \u51648 \u50896 \u51012  \u45347 \u50612 \u48400 \u46020  \u49436 \u47448 \u51204 \u54805 \u50640 \u49436  \u53448 \u46973 , \u46608  \u53448 \u46973 \u51060 \u45796 . \u44536 \u47092  \u46412 \u47560 \u45796  \u50500 \u47924 \u47000 \u46020  \u48520 \u54200 \u54620  \u45796 \u47532 \u44032  \u51109 \u48317 \u51064 \u44032 ? \u54616 \u45716  \u49373 \u44033 \u51060  \u49828 \u52824 \u44592 \u46020  \u54616 \u51648 \u47564 , \u54637 \u49345  \u44557 \u51221 \u51201 \u51060 \u45796 . \u44536 \u47084 \u47140 \u44256  \u54620 \u45796 . \u50616 \u51232 \u44036  \u44845  \u51088 \u49888 \u50640 \u44172 \u46020  \u48152 \u51677 \u51060 \u45716  \u45216 \u51060  \u50732  \u44144 \u46972 \u44256  \u48127 \u51004 \u47728 .\
\uc0\u47932 \u47200 , \u50689 \u50896 \u55176  \u45576 \u51012  \u44048 \u44256  \u49910 \u50632 \u45912  \u46412 \u46020  \u51080 \u50632 \u45796 . \u45796 \u49884 \u45716  \u44151 \u51648  \u47803 \u54624  \u44144 \u46972 \u45716  \u44152  \u50508 \u50520 \u51012  \u46412 . \'91\u44536 \u45285  \u51453 \u44172  \u45460 \u46160 \u51648 , \u50780  \u45216  \u49332 \u47160 \u50612 ?\'92 \u51104 \u51060  \u46308  \u46412 \u47560 \u45796  \u51060 \u45824 \u47196  \u44648 \u51648  \u50506 \u50520 \u51004 \u47732 , \u50500 \u52840 \u51060  \u50724 \u51648  \u50506 \u50520 \u51004 \u47732  \u54616 \u44256  \u47588 \u51068  \u48164  \u48716 \u50632 \u45796 . \u44536 \u45216 \u46020 , \u45576 \u51012  \u46516 \u51012  \u46412  \u50612 \u44608 \u50630 \u51060  \u52286 \u50500 \u50728  \u50500 \u52840 \u51060  \u45804 \u44049 \u51648  \u50506 \u50520 \u45796 . \u51116 \u54876 \u48337 \u50896  \u52285 \u47928 \u51012  \u53685 \u54644  \u46308 \u50612 \u50724 \u45716  \u52488 \u50668 \u47492 \u51032  \u54663 \u49332 \u51060  \u48120 \u52824 \u46020 \u47197  \u49436 \u44544 \u54160 \u45796 . \u44536 \u47088 \u45936  \u50864 \u50672 \u55176  \u50672 \u44208 \u46108  \u46972 \u46356 \u50724 \u50640 \u49436  \u51060 \u47492 \u46020  \u47784 \u47476 \u45716  \u45224 \u51088 \u44032  \u51060 \u47088  \u47568 \u51012  \u54644 \u51500 \u45796 . \u49332 \u50500 \u51080 \u50612 \u51480 \u49436  \u44256 \u47577 \u45796 \u44256 , \u49332 \u50500 \u51456  \u44163 \u47564 \u51004 \u47196 \u46020  \u45796 \u54665 \u51060 \u46972 \u44256 . \u44536 \u47084 \u45768  \u50724 \u45720 \u51008  \u49332 \u50500 \u48372 \u46972 \u44256 ... \u44536  \u47568 \u51012  \u45149 \u51004 \u47196  \u55128 \u47084 \u45208 \u50724 \u45716  \u44536 \u51032  \u51092 \u51092 \u54620  \u45432 \u47019 \u49548 \u47532 \u44032  \u44032 \u49844 \u51012  \u51201 \u49512 \u45796 . \u49324 \u44256  \u51060 \u54980  \u52376 \u51020 \u51004 \u47196  \u47560 \u51020 \u50640  \u48149 \u55180  \u50948 \u47196 \u50688 \u45796 .\
\uc0\u50620 \u47560  \u54980 , TV\u50640 \u49436  \u51061 \u49689 \u54620  \u47785 \u49548 \u47532 \u44032  \u55128 \u47084 \u45208 \u50752  \u54848 \u47536  \u46319  \u44536  \u50526 \u51004 \u47196  \u45796 \u44032 \u44052 \u45796 . \u54868 \u47732  \u49549 \u50640 \u49436  \u45576 \u48512 \u49884 \u44172  \u48731 \u51060  \u45208 \u45716  \u45224 \u51088 \u44032  \u46972 \u46356 \u50724 \u50640 \u49436  \u46308 \u50632 \u45912  \u44536  \u45432 \u47000 \u47484  \u48512 \u47476 \u45716 \u45936  \u49900 \u51109 \u51012  \u47581 \u52824 \u47196  \u44893  \u46412 \u47140  \u47582 \u51008  \u44592 \u48516 . \u51228 \u45824 \u47196  \u45909 \u53685 \u49324 \u44256 ! \u45817 \u54664 \u45796 . \u44536 \u44032  \u48148 \u47196  \u54812 \u49457 \u52376 \u47100  \u46496 \u50724 \u47476 \u45716  \u49888 \u51064 \u48180 \u46300  \u51060 \u53364 \u47549 \u49828 \u51032  \u48372 \u52972  \'91\u47448 \u49440 \u51116 \'92\u46976 \u45796 . 2009\u45380  \u44200 \u50872 . \u44536 \u47111 \u44172  \u49324 \u44256 \u52376 \u47100 , \u50868 \u47749 \u52376 \u47100  \u44536 \u50640 \u44172  \u51077 \u45909 \u54664 \u45796 . \u49440 \u51116 \u51032  \u47785 \u49548 \u47532 \u45716  \u44536 \u45376 \u51032  \u46304 \u46304 \u54620  \u48260 \u54016 \u47785 \u51060 \u50632 \u44256 , \u50676 \u47148 \u54620  \u45909 \u51656 \u51060  \u50976 \u51068 \u54620  \u45209 \u51060 \u50632 \u45796 . \u55192 \u46308  \u46412 \u47732  \u44536 \u44032  \u54644 \u51456  \u47568 \u51012  \u46496 \u50732 \u47532 \u47728  \'91\u44536 \u47000  \u50724 \u45720 \u51008  \u49332 \u50500 \u48372 \u51088 \'92 \u54616 \u47728  \u45796 \u49884  \u55192 \u51012  \u45256 \u45796 . \u44536 \u47111 \u44172  \u54616 \u47336 \u54616 \u47336  \u49332 \u45796  \u48372 \u45768  \u45796 \u49884  \u50883 \u44172  \u46104 \u44256  \u50500 \u54548 \u51060  \u47924 \u45964 \u51648 \u45716  \u45216 \u51060  \u50724 \u44596  \u50724 \u45716 \u44396 \u45208  \u49910 \u50632 \u45716 \u45936 .\
2023\uc0\u45380  1\u50900  1\u51068  0\u49884  0\u48516 . \u49440 \u51116 \u44032 ... \u51453 \u50632 \u45796 . \u44536  \u49692 \u44036  \u49556 \u51032  \u49464 \u49345 \u46020  \u47924 \u45320 \u51276 \u45796 . \u50500 \u45768 , \u47924 \u45320 \u51652  \u51460  \u50508 \u50520 \u45716 \u45936 ? \u44600 \u48148 \u45797 \u50640 \u49436  \u50633 \u50633  \u50872 \u45796 \u44032  \u51060 \u49345 \u54620  \u45712 \u45196 \u50640  \u45576 \u51012  \u46496 \u48372 \u45768 ... \u50668 \u44596 ... \u44368 \u49892 ??? \u47924 \u47140  15\u45380  \u51204 \u51004 \u47196  \u53440 \u51076 \u49836 \u47549  \u54644 \u48260 \u47160 \u45796 . \u48148 \u47196  \u49440 \u51116 \u44032  \u49332 \u50500 \u51080 \u45716 , 2008\u45380  \u45208 \u51032  \u50676 \u50500 \u54857 \u51004 \u47196 ! \u49900 \u51648 \u50612  \u44536 \u45376 \u44032  \u45796 \u47532 \u47484  \u51075 \u51008  \'91\u49324 \u44256 \'92\u44032  \u51068 \u50612 \u45208 \u44592  \u51060 \u51204  \u49884 \u51216 \u51060 \u45796 . \u51060 \u44148  \u49888 \u51060  \u51452 \u49888  \u44592 \u54924 \u44032  \u50500 \u45776 \u44620 ? \u44536 \u47111 \u45796 \u47732  \u44396 \u54624 \u44144 \u50556  \u44845 . \u49440 \u51116 \u46020 . \u45208 \u46020 . \u51088 \u49888 \u50640 \u44172  \u45797 \u52832  \u48520 \u50868 \u51032  \u49324 \u44256 \u47484  \u47561 \u44256 , \u49440 \u51116 \u50640 \u44172  \u45908  \u50724 \u47004  \u49334 \u51012  \u49332 \u44172  \u54644 \u51452 \u44256  \u49910 \u45796 .\
