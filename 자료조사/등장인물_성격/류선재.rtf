\f1\b0\fs24 \cf0 #\uc0\u54788 \u51116 (2023\u45380 )\u51032  \u49440 \u51116 \
2009\uc0\u45380  \u45936 \u48596  \u51060 \u47000  \u54788 \u51116 \u44620 \u51648 \u46020  \u51221 \u49345 \u51032  \u51088 \u47532 \u50640  \u50864 \u46749  \u49436  \u51080 \u45716  \u53457 \u48180 \u46300  \u51060 \u53364 \u47549 \u49828 \u51032  \u48372 \u52972 . \u50672 \u44592 \u51088 \u47196  \u49828 \u54169 \u53944 \u47100 \u51012  \u45331 \u54784  \u47751  \u54200 \u51032  \u50689 \u54868 , \u46300 \u46972 \u47560 \u47484  \u55176 \u53944 \u49884 \u53412 \u47728  \u48176 \u50864 \u47196 \u49436 \u46020  \u45824 \u51473 \u50640 \u44172  \u51064 \u51221 \u48155 \u51008  \u53681 \u49828 \u53440 .\
\uc0\u44284 \u44144  \u49688 \u50689 \u49440 \u49688 \u50688 \u45912  \u44536 \u45716  \u45000 \u51060  \u51340 \u51208 \u46108  \u51060 \u54980  \u52828 \u44396  \u51064 \u54785 \u51012  \u46384 \u46972  \u50724 \u46356 \u49496 \u50640  \u44052 \u45796 \u44032  \u44592 \u54925 \u49324  \u45824 \u54364  \u45576 \u50640  \u46916 \u50612  \u45936 \u48596 \u54664 \u45796 . \u45224 \u46308 \u48372 \u45796  \u49789 \u44172  \u49884 \u51089 \u54644  \u53360  \u49324 \u46993 \u51012  \u48155 \u50520 \u45796 \u45716  \u49373 \u44033 \u50640  \u46160  \u48176 \u47196  \u45908  \u45432 \u47141 \u54616 \u47728  \u49788  \u50630 \u51060  \u45804 \u47140 \u50772 \u44148 \u47564 , \u50612 \u45712  \u49692 \u44036  \u45797 \u52828  \u48264 \u50500 \u50883 . \u47784 \u46304  \u44172  \u54728 \u47924 \u54644 \u51276 \u45796 . \u51104  \u47803  \u46300 \u45716  \u48164 \u51060  \u47566 \u50500 \u51648 \u51088  \u51060 \u45824 \u47196  \u44032 \u45800  \u47924 \u45320 \u51656  \u44163  \u44057 \u50500  \u50724 \u47004  \u44256 \u48124  \u45149 \u50640  \u50672 \u50696 \u44228  \u51008 \u53748 \u47484  \u44208 \u49900 \u54664 \u45716 \u45936 . \u47560 \u51648 \u47561  \u53080 \u49436 \u53944  \u47924 \u45824 \u47484  \u47560 \u52828  \u44536 \u45216  \u48164 . \u44536 \u44032  \u44049 \u51089 \u49828 \u47112  \u49464 \u49345 \u51012  \u46496 \u45228 \u45796 . \u50616 \u47200 \u50640 \u49440  \'91\u48520 \u47732 \u51613 , \u50864 \u50872 \u51613 \u51004 \u47196  \u51064 \u54620  \u44537 \u45800 \u51201  \u49440 \u53469 \'92\u51060 \u46972 \u47728  \u52628 \u52769 \u49457  \u44592 \u49324 \u47484  \u50143 \u50500 \u45256 \u44256 , \u51204 \u44397 \u48124 \u51060  \u52649 \u44201 \u50640  \u48736 \u51276 \u45796 . \u44536 \u47088 \u45936 , \u51221 \u47568  \u44536 \u51032  \u51453 \u51020 \u51008  \u51088 \u49332 \u51060 \u50632 \u51012 \u44620 ?\
#2008\uc0\u45380  \u49440 \u51116 \
\uc0\u51088 \u44048 \u44256  \u49688 \u50689 \u48512  \u50640 \u51060 \u49828 . \u51204 \u44397 \u52404 \u51204 \u50640 \u49436  2\u44288 \u50773 , \u51452 \u51333 \u47785 \u50640 \u49440  \u48149 \u53468 \u54872 \u50640  \u44204 \u51460 \u47564 \u54620  \u44592 \u47197 \u51012  \u49464 \u50864 \u47728  \u52264 \u49464 \u45824  \u49828 \u54252 \u52768  \u49828 \u53440 \u47196  \u50616 \u47200 \u51032  \u49828 \u54252 \u53944 \u46972 \u51060 \u53944 \u47484  \u48155 \u51012 ... \u48852 \u54664 \u51004 \u45208 ! \u50612 \u44648  \u49688 \u49696 \u51012  \u48155 \u44256  \u51116 \u54876 \u54616 \u45716  \u48148 \u46988 \u50640  \u53444 \u53444 \u45824 \u47196  \u51068  \u51460  \u50508 \u50520 \u45912  \u49440 \u49688  \u49373 \u54876 \u51060  \u51104 \u49884  \u51452 \u52644 \u54616 \u44256  \u51080 \u45796 .\
\uc0\u46497  \u48268 \u50612 \u51652  \u50612 \u44648 , \u50868 \u46041 \u51004 \u47196  \u45796 \u51256 \u51652  \u45800 \u45800 \u54620  \u47800 , 365\u51068  \u50868 \u46041 \u48373 \u47564  \u44152 \u52824 \u44256  \u45796 \u45376 \u46020  \u44051 \u48317 \u54620  \u54588 \u51648 \u52972 , \u51096 \u45212  \u50620 \u44404 \u50640  \u44992 \u48372 \u51060 \u49828 \u44620 \u51648 . \u45796  \u44032 \u51652  \u44163  \u44057 \u51008 \u45936  \u51032 \u50808 \u47196  \u51064 \u44592 \u44032  \u50630 \u45796 ?! \u49324 \u51060 \u50900 \u46300  \u50620 \u51697 \u46308 \u51060  \u51064 \u44592 \u47484  \u55081 \u50424 \u45912  \u44536 \u46412  \u44536  \u49884 \u51208 . 365\u51068  \u50868 \u46041 \u48373  \u52264 \u47548 \u51004 \u47196  \u52404 \u50977 \u44288 \u47564  \u46308 \u46973 \u44144 \u47532 \u45912  \u44536 \u44032  \u50668 \u54617 \u49373 \u46308 \u51032  \u45576 \u50640  \u46916 \u50632 \u51012  \u47532 \u44032 . \u49345 \u45224 \u51088  \u44057 \u44256  \u47924 \u46749 \u46749 \u54644  \u48372 \u50668  \u49789 \u44172  \u45796 \u44032 \u44032 \u44592  \u55192 \u46304  \u49828 \u53440 \u51068 \u52376 \u47100  \u48372 \u51060 \u51648 \u47564  \u49549 \u51221  \u47566 \u44256  \u47560 \u51020 \u51060  \u50557 \u54616 \u45796 . \u54728 \u45817 \u49828 \u47101 \u44256  \u49692 \u51652 \u54620  \u44396 \u49437 \u51060  \u51080 \u50612  \u49324 \u49892 \u49345  \u49692 \u54620  \u45824 \u54805 \u44204 \u45224 . \u47672 \u47532 \u50752  \u44032 \u49844 \u50640  \u49688 \u50689 \u44284  \u44032 \u51313 \u48150 \u50640  \u50630 \u50632 \u45912  \u44536 \u50640 \u44172  \u49352 \u47196 \u50868  \u44163 \u51060  \u46308 \u50612 \u50728 \u45796 .\
\uc0\u48148 \u47196  \'91\u52395 \u49324 \u46993 \'92 \u49688 \u50689 \u51008  0.01\u52488 \u47196  \u49849 \u48512 \u44032  \u44040 \u47532 \u45716  \u44592 \u47197  \u49912 \u50880 \u51060 \u44592 \u50640  \u44048 \u51221 \u51032  \u46041 \u50836 \u47484  \u52572 \u49548 \u54620 \u51004 \u47196  \u54644 \u50556  \u54620 \u45796 . \u46412 \u47928 \u50640  \u54217 \u51221 \u49900 \u51060  \u50500 \u51452  \u51473 \u50836 \u54616 \u44256  \u44536 \u44163 \u51060  \u44263  \u51088 \u49888 \u51032  \u44053 \u51216 \u51060 \u46972  \u51088 \u49888 \u54624  \u49688  \u51080 \u45796 . \u44540 \u45936 ... \u51060 \u44172  \u51060 \u47111 \u44172  \u49789 \u44172  \u47924 \u45320 \u51648 \u45716  \u44144 \u50688 \u45208 ?!\
\uc0\u49548 \u45208 \u44592 \u44032  \u45236 \u47532 \u45912  \u45216 , \u50526 \u51665 \u50640  \u49324 \u45716  \u51076 \u49556 \u51060 \u46972 \u45716  \u50668 \u51088 \u50528 \u44032  \u45432 \u46976  \u50864 \u49328 \u51012  \u50444 \u50892 \u51456  \u49692 \u44036  \u49900 \u51109 \u51060  \u53237 . \u46504 \u50612 \u51276 \u45796 . \u52395 \u45576 \u50640  \u48152 \u54620  \u51060 \u54980  \u54217 \u51221 \u49900 \u51060 \u44256  \u47952 \u44256  \u50756 \u51204 \u55176  \u54168 \u51060 \u49828 \u47484  \u51075 \u50632 \u45796 . \u50668 \u51088 \u50528  \u50620 \u44404  \u54620 \u48264  \u48372 \u44192 \u45796 \u44256  \u50689 \u54868 \u46020  \u50504  \u48372 \u47732 \u49436  \u44536  \u50528 \u44032  \u51080 \u45716  \u48708 \u46356 \u50724  \u44032 \u44172  \u50526 \u51012  \u47588 \u51068  \u49436 \u49457 \u45824 \u45716  \u44852 \u51060 \u46976 .\
