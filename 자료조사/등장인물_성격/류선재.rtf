\uc0\u46504 \u47140 \u49436  \u47568  \u54620 \u48264  \u47803  \u48537 \u50668 \u48376  \u51677 \u49324 \u46993  \u44536 \u45376 \u44032  \u45712 \u45815 \u50630 \u51060  \u54616 \u45720 \u50640 \u49436  \u46609  \u46504 \u50612 \u51652  \u44163 \u52376 \u47100  \u49688 \u50689 \u51109 \u50640  \u45208 \u53440 \u45208 \u49440  \u45253 \u45796  \u45804 \u47140 \u50752  \u50504 \u44596 \u45796 .\
\uc0\u44536 \u47084 \u45908 \u45768  \u49436 \u47101 \u44172 \u46020  \u50872 \u47732 \u49436  \u45216  \u49324 \u46993 \u54620 \u45800 \u45796 . \u50612 \u51228 \u44620 \u51648  \u45236  \u51316 \u51116 \u51312 \u52264  \u47784 \u47476 \u45912  \u49556 \u51060  \u50612 \u46523 \u44172  \u45236  \u51060 \u47492 \u51012  \u50500 \u45716  \u44148 \u51648 . \u50780  \u51088 \u49888 \u51012  \u49836 \u54536  \u45576 \u51004 \u47196  \u48372 \u45716  \u44148 \u51648 . \u49373 \u44033 \u54644  \u48372 \u47732  \u51060 \u49345 \u54620  \u44396 \u49437 \u51060  \u54620 \u46160  \u44060 \u44032  \u50500 \u45772 \u45936  \u44536 \u47088 \u45936 \u46020  \u50780  \u51216 \u51216  \u45908  \u51339 \u50500 \u51648 \u45716  \u44148 \u51648 .\
